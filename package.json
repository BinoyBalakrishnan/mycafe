{
  "name": "mycafe",
  "version": "1.0.0",
  "description": "Full stack React + Node.js app for restaurant menu",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "install-client": "cd client && npm install",
    "build-client": "cd client && npm run build",
    "postinstall": "echo 'Skipping client build on Azure'",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.21.0",
    "jsonwebtoken": "^9.0.2",
    "mssql": "^10.0.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.13",
    "path": "^0.12.7",
    "twilio": "^5.3.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.4"
  },
  "engines": {
    "node": "22.x",
    "npm": "10.x"
  }
}
